	;;
    yes)
	check_dir=$srcdir
	AV_ALG_INCLUDES='-I$(top_srcdir)'
	AV_ALG_LIBS='$(top_srcdir)/anti-virus/libkld.a'
	cat >> confdefs.h <<\EOF
