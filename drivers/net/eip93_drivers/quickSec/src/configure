EOF

	;;
    *)
	check_dir=$withval
	AV_ALG_INCLUDES=-I$check_dir
	AV_ALG_LIBS=$check_dir/anti-virus/libkld.a
	cat >> confdefs.h <<\EOF
#define WITH_AV_ALG 1
EOF

	;;
  esac

  if test "$check_dir" = ""
  then
	AV_ALG_INCLUDES=""
	AV_ALG_LIBS=""
