AUTOMAKE_OPTIONS = 1.0 foreign dist-zip no-dependencies
INCLUDES = -I$(srcdir)/.. -I$(top_srcdir)  -I$(top_srcdir)/lib/sshutil/sshcore  -I$(top_srcdir)/lib/sshutil/sshadt  -I$(top_srcdir)/lib/sshutil/ssheloop  -I$(top_srcdir)/lib/sshutil/sshstrutil  -I$(top_srcdir)/lib/sshutil/sshfsm 
LDADD = $(top_builddir)/lib/sshutil/sshfsm/libsshfsm.a $(top_builddir)/lib/sshutil/sshstrutil/libsshstrutil.a $(top_builddir)/lib/sshutil/ssheloop/libssheloop.a $(top_builddir)/lib/sshutil/sshadt/libsshadt.a $(top_builddir)/lib/sshutil/sshcore/libsshcore.a 

WINCONF_OPTIONS = maintainer=ylo@ssh.fi

noinst_LIBRARIES=libsshstream.a

EXTRA_DIST =

libsshstream_a_SOURCES =        \
        sshstream.c             \
        sshstreamconnect.c      \
        sshdatastream.c         \
        sshstreampair.c         \
        sshfilterstream.c       








public_headers +=                       \
        sshstreamconnect.h              \
        sshdatastream.h                 \
        sshstream.h                     \
        sshstreampair.h                 \
        sshfilterstream.h               








GRANDLIB_OBJECTS = $(libsshstream_a_OBJECTS)

include $(top_srcdir)/Makefile.libextra
include $(top_srcdir)/Makefile.grandlib_provider
